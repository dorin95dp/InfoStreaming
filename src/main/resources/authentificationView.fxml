<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane onKeyPressed="#enterPressed" prefHeight="317.0" prefWidth="422.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.faf.streaming.controllers.StartViewController">
    <children>
        <TextField fx:id="username" layoutX="128.0"  layoutY="109.0" promptText="Username" />
        <PasswordField fx:id="password" layoutX="128.0" layoutY="151.0" promptText="Password" />
        <Button layoutX="125.0" layoutY="204.0" mnemonicParsing="false" onAction="#login" text="Connect" />
        <Button layoutX="200.0" layoutY="204.0" mnemonicParsing="false" onAction="#changeSceneToRegister" text="Register" />
        <Label fx:id="lbStatus" layoutX="140.0" layoutY="62.0" textAlignment="CENTER" />

        <TextField fx:id="serverIp" layoutX="128.0" layoutY="59.0" promptText="Server IP" />
    </children>
</AnchorPane>
